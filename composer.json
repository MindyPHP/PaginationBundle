{
    "name": "mindy/pagination-bundle",
    "type": "symfony-bundle",
    "keywords": [
        "pagination",
        "pager"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Falaleev Maxim",
            "email": "max@studio107.ru"
        }
    ],
    "require": {
        "php": ">=7.1",
        "mindy/pagination": "~4.0",
        "symfony/config": "~3.0|~4.0",
        "symfony/dependency-injection": "~3.0|~4.0",
        "symfony/http-kernel": "~3.0|~4.0",
        "symfony/yaml": "~3.0|~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "symfony/framework-bundle": "^3.4",
        "symfony/routing": "~3.0|~4.0",
        "symfony/twig-bundle": "~3.0|~4.0",
        "symfony/var-dumper": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Mindy\\Bundle\\PaginationBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mindy\\Bundle\\PaginationBundle\\Tests\\": ""
        }
    }
}
